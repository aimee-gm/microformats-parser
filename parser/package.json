{
  "name": "microformats-parser",
  "version": "1.3.1",
  "description": "A JavaScript microformats v2 parser for the browser and node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://aimee-gm.github.io/microformats-parser/",
  "repository": "https://github.com/aimee-gm/microformats-parser.git",
  "author": "Aimee Gamble-Milner (https://github.com/aimee-gm)",
  "license": "MIT",
  "keywords": [
    "microformats",
    "parser",
    "mf2",
    "indieweb"
  ],
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha --require ts-node/register test/**/*.spec.ts",
    "test:package": "mocha test/package.spec.js"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "/dist",
    "/README.md",
    "/LICENSE",
    "/CONTRIBUTING.md"
  ],
  "dependencies": {
    "parse5": "^6.0.0"
  },
  "devDependencies": {
    "microformat-tests": "https://github.com/microformats/tests",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "chai": "^4.2.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12",
    "@types/parse5": "^5.0.2",
    "glob": "^7.1.6"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "src/**"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  }
}
